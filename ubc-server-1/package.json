  "description": "Express server with Apollo GraphQL middleware",
  "main": "index.js",
  "private": true,
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nodemon src/index.ts --watch src --exec ts-node --allowJs",
