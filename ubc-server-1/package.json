    "@types/express": "^4.0.36",
    "@types/express-handlebars": "0.0.29",
    "@types/graphql": "^0.8.6",
    "@types/isomorphic-fetch": "0.0.30",
    "@types/lodash": "^4.14.34",
    "@types/multer": "0.0.33",
    "@types/node": "^6.0.65",
    "@types/source-map": "^0.5.0",
    "@types/winston": "^2.3.0",
    "app-root-path": "^2.0.1",
    "casual": "^1.5.14",
    "cors": "^2.8.3",
    "proxyquire": "^1.7.10",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.6",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.2",
    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "typescript": "^2.4.0"
