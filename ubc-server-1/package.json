  "dependencies": {
    "apollo-errors": "^1.3.0",
    "assert": "^1.4.1",
    "body-parser": "1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "dataloader": "1.3.0",
    "express": "4.15.2",
    "express-session": "1.15.1",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-subscriptions": "^0.3.0",
    "graphql-tools": "^0.10.1",
    "helmet": "^3.4.0",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
